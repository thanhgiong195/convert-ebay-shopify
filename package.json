{
  "name": "convert-csv",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "electron:mac": "electron-builder --mac",
    "electron:win": "electron-builder --win",
    "electron:linux": "electron-builder --linux",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "convert-csv",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "portable": {
      "unicode": false,
      "artifactName": "convert-csv.exe"
    }
  },
  "author": "thanhgiong",
  "devDependencies": {
    "electron": "^15.0.0",
    "electron-builder": "^22.13.1"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "beepbeep": "^1.3.0",
    "bootstrap": "^5.2.3",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.3.3",
    "csv-stringify": "^6.2.3"
  }
}
